# Supabase Configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Nudge Configuration
# Threshold in minutes for considering a message "stale" (default: 180 = 3 hours)
THRESHOLD_MINUTES=180

# Logging Configuration
LOG_LEVEL=INFO

# Dry Run Mode (set to true for testing without creating actual events)
DRY_RUN=false
